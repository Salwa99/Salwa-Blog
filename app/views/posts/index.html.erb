  <%= render "shared/user", user: @user %>
    
    <% if @posts.posts.empty? %>
        <p class="not-available">No post available</p>
    <% end %>
    <% @posts.posts.each do |post| %>
        <%= render "shared/post", post: post %>
        <%= render "shared/comment", comments: post.last_five_comments%>
    <% end %>

    <button class="link-btn" type="button">Pagination</button>

</section>
